{
	"name": "radeir/sdk",
	"description": "Rade SDK Seamlessly integrate secure and reliable banking services into your application.",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Omid Asgari",
			"email": "asgarihope@gmail.com"
		}
	],
	"scripts": {
		"stan": "phpstan analyse",
		"refactor": "rector process",
		"test": "phpunit",
		"coverage": "./vendor/bin/phpunit --coverage-html coverage"
	},
	"require": {
		"php": ">=8.1",
		"guzzlehttp/guzzle": "~7.0"
	},
	"autoload": {
		"psr-4": {
			"Radeir\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require-dev": {
		"phpstan/phpstan": "2.1.22",
		"rector/rector": "2.1.2",
		"larapack/dd": "^1.1",
		"phpunit/phpunit": "10.5.52"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Radeir\\Provider\\RadeServiceProvider"
			],
			"aliases": {
				"Rade": "Radeir\\Facade\\Rade"
			}
		}
	}
}
